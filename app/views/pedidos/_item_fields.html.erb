<div class="nested-fields">
  <div class="field">
    <%= f.label :produto %>
    <%= f.collection_select(:produto_id, Produto.all, :id, :nome, { prompt: "" }, { class: 'browser-default' }) %>
  </div>

  <div class="field">
    <%= f.label :quantidade %>
    <%= f.text_field :quantidade, id: :item_quantidade %>
  </div>
 
  <div class="field">
    <%= f.label :valor %>
    <%= f.text_field :valor, id: :item_valor %>
  </div>
 
  <%= link_to_remove_association "Remover Item", f %>
 
  <br><br><br>
</div>